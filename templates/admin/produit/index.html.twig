{% block body %}
    <table class="table table-striped table-light mt-4 border border-secondary-rounded text-center">
        <table class="table table-striped table-light mt-4 border border-secondary-rounded text-center">
            <thead>
            <tr>
                <th><button class="btn btn-dark btn-md btn-block ">Groupes</button></th>
                <th><button class="btn btn-dark btn-md btn-block">Id Substance</button></th>
                <th><button class="btn btn-dark btn-md btn-block">Nb test item</button></th>
                <th><button class="btn btn-dark btn-md btn-block">Voie admin</button></th>
                <th><button class="btn btn-dark btn-md btn-block">Date 1er prélèvement</button></th>
                <th><button class="btn btn-dark btn-lg btn-block"></button></th>
            </tr>
            </thead>
        <tbody>
        {% for produit in etude.produits %}
            <tr>
                <td><a href="{{ path('groupe.index', {id: produit.id, slug: produit.slug}) }}">{{ produit.groupe }}</a></td>
                <td>{{ produit.idProduitPorsolt }}</td>
                <td>{{ produit.nbreAnimaux }}</td>
                <td>{{ produit.voieAdmin }}</td>
                <td>{{ produit.datePremierPrelevement|date("d/m/Y") }}</td>
                <td><a href="{{ path('admin.produit.edit', {id: produit.id}) }}" class="btn btn-secondary">Editer</a></td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    <div class="container">
    <a href="{{ path('admin.produit.new') }}" class="btn btn-primary">creer</a>
    </div>
{% endblock %}

